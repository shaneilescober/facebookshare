<?xml version="1.0" encoding="utf-8"?>
<application>
    <langs support="en" default="en"></langs>
    <api use="on">
        <ifs>
            <if class="apiBuilderLib" permission="off" desc="Api for the builder library"></if>
            <if class="apiGetSetting" permission="off" desc="Api for getting settings of front"></if>
        </ifs>
    </api>
    <admin use="on" index="adminPageSettings">
        <ifs>
            <if class="adminExecIndex" permission="off" desc="Add logic"></if>
            <if class="adminPageSettings" permission="off" desc="Settings page"></if>
            <if class="adminPageManage" permission="off" desc="Sequence manage page"></if>
            <if class="adminPageAdvanced" permission="off" desc="Advanced page"></if>
        </ifs>
        <menus>
        </menus>
    </admin>
    <front use="on">
        <ifs>
            <if class="frontPageFbShare" permission="off" code-assist="on" desc="Front for showing the facebook share button"></if>
            <if class="frontPageNoRecord" permission="off" code-assist="on" desc="Front for when no data is saved in database"></if>
        </ifs>
    </front>
</application>
